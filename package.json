    "babel-preset-es2017": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.22.0",
    "babel-runtime": "^6.20.0",
    "cli": "^1.0.1",
    "colors": "^1.1.2",
    "node-status": "^1.0.0",
    "pg": "^6.1.2",
    "pg-cursor": "^1.0.1"
  }
